    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "source-map-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
